{
  "name": "angular-redhawk",
  "version": "1.0.0",
  "description": "AngularJS library for interfacing with REDHAWK SDR via Geon's fork of REST-python",
  "scripts": {
    "build": "gulp && rollup -c",
    "clean": "rimraf dist coverage .nyc_output",
    "lint": "tslint src/**/*.ts",
    "test": "nyc ava --reporter=text --reporter=html",
    "upload-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prebuild": "npm run clean",
    "pretest": "cross-env NODE_ENV=test npm run build",
    "posttest": "npm run lint",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "postinstall": "mv dist/* . && rimraf dist"
  },
  "author": "Thomas Goodwin <btgoodwin@geontech.com>",
  "license": "GPL-3.0",
  "contributors": [
    "Patrick Wolfram <pwolfram@geontech.com>",
    "Sirajuddin Choudhary <sirajuddin.c@gmail.com> (https://sirajc.github.io/angular2-bs4-navbar)",
    "Max Davidson (https://github.com/maxdavidson/typescript-library-boilerplate.git"
  ],
  "repository": "git://github.com/GeonTech/angular-redhawk.git",
  "dependencies": {
    "@angular/common": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/platform-browser": "^2.1.2",
    "cross-env": "^3.1.3",
    "gulp-inline-ng2-template": "^3.0.2",
    "gulp-typescript": "^3.1.2",
    "nyc": "^8.3.2",
    "rxjs": "5.0.0-beta.12",
    "typescript": "^2.0.6",
    "typings": "^1.5.0",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "change-case": "^3.0.0",
    "coveralls": "^2.11.14",
    "cross-env": "^3.1.1",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^3.0.1",
    "gulp-typescript": "^3.0.2",
    "nyc": "^8.3.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.1",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.6",
    "typings": "^1.4.0"
  },
  "peerDependencies": {
    "@angular/common": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/platform-browser": "^2.1.2",
    "rxjs": "5.0.0-beta.12",
    "typescript": "^2.0.6"
  },
  "files": [
    "dist"
  ],
  "main": "dist/src/angular-redhawk.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=6.6.0",
    "npm": ">=3.10.0"
  }
}
