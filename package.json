{
  "name": "angular-redhawk",
  "version": "0.0.0",
  "description": "Angular library for interfacing with REDHAWK SDR via Geon's fork of REST-python",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n angular-redhawk -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "version": "npm run version:set && npm run version:add",
    "version:set": "sed -i -E 's/(\"version\":[[:space:]]*\").+(\")/\\1'$npm_package_version'\\2/g' src/package.json",
    "version:add": "git add src/package.json",
    "lint": "npm run lint:base",
    "lint:base": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lint:fix": "npm run lint:base -- --fix",
    "test": "tsc && karma start"
  },
  "author": "Thomas Goodwin <btgoodwin@geontech.com>",
  "license": "GPL-3.0",
  "keywords": [
    "angular",
    "redhawk-sdr"
  ],
  "contributors": [
    "Patrick Wolfram <pwolfram@geontech.com>",
    "Adam Heacock <aheacock@geontech.com>",
    "Trevor Plumley <tplumley@geontech.com>"
  ],
  "repository": "git://github.com/GeonTech/angular-redhawk.git",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "angular-in-memory-web-api": "^0.3.2",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.15.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.2",
    "protractor": "~5.1.0",
    "rollup": "^0.49.3",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "zone.js": "^0.8.4"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
