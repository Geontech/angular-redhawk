{
  "name": "angular-redhawk",
  "version": "1.0.0",
  "description": "AngularJS library for interfacing with REDHAWK SDR via Geon's fork of REST-python",
  "main": "dist/angular-redhawk.js",
  "scripts": {
    "clean": "rm -rf dist",
    "html": "find . -type f -name '*.html' ! -path './node_modules/*' ! -path './typings/*' ! -path './dist/*' -exec cp --parents {} ./dist \\;",
    "prepublish": "npm run clean && tsc && npm run html",
    "postinstall": "typings install",
    "tslint": "tslint -c tslint.json angular-redhawk.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Thomas Goodwin <btgoodwin@geontech.com>",
  "license": "GPL-3.0",
  "contributors": [
    "Patrick Wolfram <pwolfram@geontech.com>",
    "Sirajuddin Choudhary <sirajuddin.c@gmail.com> (https://sirajc.github.io/angular2-bs4-navbar)"
  ],
  "repository": "git://github.com/GeonTech/angular-redhawk.git",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "rxjs": "^5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "tsconfig-glob": "^0.4.3",
    "tslint": "^3.15.1",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  },
  "typings": "dist/angular-redhawk.d.ts",
  "engines": {
    "npm": ">=3.10.0"
  }
}
